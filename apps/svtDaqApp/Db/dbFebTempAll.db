record(calc, SVT:lv:hyb:all:stat:curr)
{
	field(SCAN,"Passive")
	field(INPA,"SVT:lv:hyb:top:stat:curr CPP")
	field(INPB,"SVT:lv:hyb:bot:stat:curr CPP")
	field(CALC,"(A > 0) && (B > 0)")
}