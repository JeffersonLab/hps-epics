record(calc, SVT:lv:hyb:1t:stat:curr)
{
	field(SCAN,"Passive")
	field(INPA,"SVT:lv:fe:9:anap:i_rd CPP")
	field(INPB,"SVT:lv:fe:9:anap:i_rd.LOW CPP")
	field(CALC,"(A > B)")	
