
record(sub,SVT:poll_xml)
{
    field(SCAN,"5 second")
    field(INAM,"subPollInit")
    field(SNAM,"subPollProcess")
    field(FLNK,"SVT:poll_stat")
    #field(INPA, "SVT:poll_xml_ip")
    #field(INPB, "SVT:poll_xml_port")
}


record(sub, SVT:poll_stat) {
    field(SCAN,"Passive")
    field(INAM,"subPollStatInit")
    field(SNAM,"subPollStatProcess")
    field(FLNK,"SVT:hb_check")
}

record(stringin, SVT:poll_xml_ip) {
    field(VAL,"localhost")
}

record(longin, SVT:poll_xml_port) {
    field(VAL,"8090")
}

record(ai, SVT:hb_check) {
  field(INP, "SVT:poll_stat")
  field(DTYP,"Soft Channel")
  field(FLNK,"SVT:process")
}

record(fanout, SVT:process)
{
  field(LNK1,"SVT:temp:fe:0:axixadc:t_rd")
  field(LNK2,"SVT:lv:fanout")
  field(LNK3,"SVT:lv:0:0:stat")
  field(LNK4,"SVT:temp:hyb:0:0:temp0:t_rd")
}



