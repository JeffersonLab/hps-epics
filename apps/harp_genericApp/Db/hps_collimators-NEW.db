record(calc, "hps_collimator_at_home") {
	field(INPA, "hps_collimator.RBV")
        field(INPB, "hps_collimator_pos_home")
	field(SCAN, ".2 second")
	field(CALC, "ABS(A-0.0)<0.1?1:0")
}
