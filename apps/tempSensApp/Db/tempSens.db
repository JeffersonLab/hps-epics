record(ai,"$(P)$(CHAN):RAW") {
    field(DESC, "Omega Temperature")
    field(DTYP, "stream")
    field(INP,  "@omegaD5311.proto getTemp($CHAN) $(PORT)")
    field(EGU,  "F")
    field(PREC, "$(PREC)")
    field(SCAN, "$(SCAN)")
    field(FLNK, "$(P)$(CHAN):CAL")
}

record(calc, "$(P)$(CHAN):CAL") {
    field(CALC, "A*B+C")
    field(INPA, "$(P)$(CHAN):RAW")
    field(INPB, "1.0")
    field(INPC, "0.0")
    field(EGU,  "F")
    field(PREC, "$(PREC)")
    field(FLNK, "$(P)$(CHAN)_F")
}

record(ai, "$(P)$(CHAN)_F") {
	alias($(P)$(CHAN))
	field(INP, "$(P)$(CHAN):CAL")
    field(EGU,  "F")
    field(PREC, "$(PREC)")
    field(FLNK, "$(P)$(CHAN)_C")
}

record(calc, "$(P)$(CHAN)_C") {
    field(CALC, "(A-32)*(5/9)")
    field(INPA, "$(P)$(CHAN):RAW")
    field(EGU,  "C")
    field(PREC, "$(PREC)")
}
